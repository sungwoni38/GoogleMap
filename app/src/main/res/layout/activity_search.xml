<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SearchActivity">

    <SearchView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="@+id/h_gline_2_1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <HorizontalScrollView
        android:id="@+id/horizontalScrollView"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        app:layout_constraintBottom_toTopOf="@+id/h_gline_2_3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/h_gline_2_1">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:weightSum="5">

            <LinearLayout
                android:layout_width="70dp"
                android:layout_height="50dp"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/img_korea"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_gravity="center"
                    android:src="@drawable/korea" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:text="한식" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="70dp"
                android:layout_height="50dp"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/img_italy"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_gravity="center"
                    android:src="@drawable/italy" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:text="양식" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="70dp"
                android:layout_height="50dp"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/img_japan"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_gravity="center"
                    android:src="@drawable/japan" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:text="일식" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="70dp"
                android:layout_height="50dp"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/img_china"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_gravity="center"
                    android:src="@drawable/china" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:text="중식" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="70dp"
                android:layout_height="50dp"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/img_bunsik"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_gravity="center"
                    android:src="@drawable/bunsik" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:text="분식" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="70dp"
                android:layout_height="50dp"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/img_drink"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_gravity="center"
                    android:src="@drawable/drink" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:text="카페" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="70dp"
                android:layout_height="50dp"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/img_bread"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_gravity="center"
                    android:src="@drawable/bread" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:text="제과" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="70dp"
                android:layout_height="50dp"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/img_fastfood"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_gravity="center"
                    android:src="@drawable/fastfood" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:text="패스트푸드" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="70dp"
                android:layout_height="50dp"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/img_shop"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_gravity="center"
                    android:src="@drawable/shop" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:text="기타" />
            </LinearLayout>


        </LinearLayout>

    </HorizontalScrollView>


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/h_gline_2_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.1" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/h_gline_2_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.2" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/h_gline_2_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.9" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="284dp"
        android:text="검색화면"
        app:layout_constraintEnd_toStartOf="@+id/v_gline_2_3"
        app:layout_constraintHorizontal_bias="0.466"
        app:layout_constraintStart_toStartOf="@+id/v_gline_2_2"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/bottom_home"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:src="@drawable/ic_home"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/v_gline_2_1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/h_gline_2_2" />

    <TextView
        android:id="@+id/txt_home"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="홈"
        android:textSize="10sp"
        app:layout_constraintEnd_toStartOf="@+id/v_gline_2_1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bottom_home" />

    <ImageView
        android:id="@+id/bottom_like"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:src="@drawable/ic_like"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/v_gline_2_2"
        app:layout_constraintStart_toStartOf="@+id/v_gline_2_1"
        app:layout_constraintTop_toTopOf="@+id/h_gline_2_2" />

    <TextView
        android:id="@+id/txt_like"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:text="즐겨찾기"
        android:textSize="10sp"
        app:layout_constraintEnd_toStartOf="@+id/v_gline_2_2"
        app:layout_constraintStart_toStartOf="@+id/v_gline_2_1"
        app:layout_constraintTop_toBottomOf="@+id/bottom_like" />

    <ImageView
        android:id="@+id/bottom_search"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:src="@drawable/ic_search"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/v_gline_2_3"
        app:layout_constraintStart_toStartOf="@+id/v_gline_2_2"
        app:layout_constraintTop_toTopOf="@+id/h_gline_2_2" />

    <TextView
        android:id="@+id/txt_search"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="검색"
        android:textSize="10sp"
        app:layout_constraintEnd_toStartOf="@+id/v_gline_2_3"
        app:layout_constraintStart_toStartOf="@+id/v_gline_2_2"
        app:layout_constraintTop_toBottomOf="@+id/bottom_search" />

    <ImageView
        android:id="@+id/bottom_card"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:src="@drawable/ic_baseline_credit_card_24"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/v_gline_2_4"
        app:layout_constraintStart_toStartOf="@+id/v_gline_2_3"
        app:layout_constraintTop_toTopOf="@+id/h_gline_2_2" />

    <TextView
        android:id="@+id/txt_card"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="카드조회"
        android:textSize="10sp"
        app:layout_constraintEnd_toStartOf="@+id/v_gline_2_4"
        app:layout_constraintStart_toStartOf="@+id/v_gline_2_3"
        app:layout_constraintTop_toBottomOf="@+id/bottom_card" />


    <ImageView
        android:id="@+id/bottom_more"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:src="@drawable/ic_more"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/v_gline_2_4"
        app:layout_constraintTop_toTopOf="@+id/h_gline_2_2" />

    <TextView
        android:id="@+id/txt_more"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="더보기"
        android:textSize="10sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/v_gline_2_4"
        app:layout_constraintTop_toBottomOf="@+id/bottom_more" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/v_gline_2_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.2" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/v_gline_2_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.4" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/v_gline_2_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.6" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/v_gline_2_4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.8" />

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/fragment_korea"
        android:layout_width="match_parent"
        android:layout_height="500dp"
        app:layout_constraintBottom_toTopOf="@+id/h_gline_2_2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/h_gline_2_3" >

    </androidx.fragment.app.FragmentContainerView>

</androidx.constraintlayout.widget.ConstraintLayout>